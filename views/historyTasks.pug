extends layout

block content
  h1= title
  h2 See tasks:
  table
    tr 
      th Start Date
      th Title
      th Description
      th Deadline
      th Deleted date
      th Completed date

    each task in tasks
      tr
        td #{task.startDate}
        td #{task.title}
        td #{task.description}
        td #{task.deadline}
        td #{task.deletedDate}
        td #{task.completionDate}
